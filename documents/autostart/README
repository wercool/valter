1) sudo apt-get install upstart
2) sudo vi /etc/init/expclient.conf
3)

#!upstart
description "expclient"

# When to start the service
start on runlevel [2345]

# Start job via the daemon control script. Replace "__username__" with your username.
exec su -l __username__ -c  'export DISPLAY=:0; cd /home/maska/git/valter/software/computer/client/qt/build-expclient-Desktop-Debug; ./expclient'

# Restart the process if it dies with a signal
# or exit code not given by the 'normal exit' stanza.
respawn

# Give up if restart occurs 10 times in 90 seconds.
respawn limit 10 90

4) sudo vi /etc/udev/rules.d/55-at91sam7s-com.rules
5)
ATTRS{idVendor}=="03eb", ATTRS{idProduct}=="6125", MODE="777", GROUP="__groupname__"
6) reboot