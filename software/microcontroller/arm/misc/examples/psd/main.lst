1343:lib_AT91SAM7S64.h **** {
1344:lib_AT91SAM7S64.h **** 	if (ms > 2000)
1345:lib_AT91SAM7S64.h **** 		return 1;   // AT91C_TIME_OUT_OF_RANGE
